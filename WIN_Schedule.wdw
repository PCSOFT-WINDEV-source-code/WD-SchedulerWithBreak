         // Left button down = a move is in progress
         gbMoveInProgress = True
         
         // Retrieves the identifier (gLink) of the clicked resource
         sResourceID is string = SchedulerInfoXY(MySelf, schResource, MouseXPos(), MouseYPos())
         
         // If the click is performed on the resource area
         if MouseXPos() <= gnResourceWidth _and_ MouseYPos() > gnDayHeight then
         	
         	// According to the type of resource (break or not) and to the collapse status
         	switch SchedulerResourceStatus(MySelf..FullName, sResourceID)
         	
         		// It is a group ("break")
         		case resoCollapsed, resoExpanded
         			// Reverses the collapse status
         			SchedulerReverseResourceStatus(SCH_SchedulerBreak..FullName, sResourceID)
         
         			// Stores the resource currently displayed (to reposition the scheduler on it)
         			gaaDisplayedResource = SchedulerPositionResource(SCH_SchedulerBreak)
         			
         			// Fills the scheduler again
         			FillSchedule()
         			
         		// It is a person ("non break" resource)
         		case resoError
         			
         			// Searches for the person
         			HReadSeekFirst(PERSON, PERSONID, ExtractString(sResourceID, 2, ESC))
         			if HFound(PERSON) then
         				
         				// Displays the phone number of the person
         				Info(PERSON.NAME +CR+ "Phone: "+PERSON.PHONE)
         				
         			end
         			
         	end
         	
         end
        type : 66049
      -
