  procedures :
   -
     name : FillSchedule
     internal_properties : CAAAAAgAAABa63Xsu0g6Rers3bQzpRfAUGdyr0IhnXw1GVSYbE0IJWKdTbNDFa0sfWiNZWwEJqeoHbGySspEO4pwLZEYk7IlqH4u56F/bpF+yV9UfDPIzRI/Vv0SpTqv/R0aUbyzQP9qt9ncsxAPVhMWfmWB
     procedure_id : 2014685149002634644
     type_code : 14
     code : |1+
      // Summary: Fills the schedule
      // Syntax:
      //FillSchedule ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      Procédure FillSchedule()
      
      sGroupID 				is string
      sPersonID 			is string
      sDisplayedResource		is string
      
      
      // Stops the display of control (to avoid the blinking effect during the fill operation)
      SCH_SchedulerBreak..DisplayEnabled = False
      
      // Stores the resource currently displayed (to reposition the scheduler on it)
      sDisplayedResource = SchedulerPositionResource(SCH_SchedulerBreak)
      
      // Clears the entire scheduler
      SchedulerDeleteAll(SCH_SchedulerBreak)
      
      // Browses the groups
      for each GROUP on STCGROUP
      	
      	// The group is identified by G[ESC]<GROUPID>
      	sGroupID = "G" +ESC+ GROUP.GROUPID
      	
      	// Adds the group
      	SchedulerAddBreakResource(SCH_SchedulerBreak..FullName, gLink(sGroupID) + GROUP.STCGROUP, (CBOX_BreakColor ? GetColor(GROUP.GROUPID) else *))
      	
      	// Adds the persons of this group
      	for each PERSON on NAME where "GroupID = " + GROUP.GROUPID
      		
      		// The person is identified by P[ESC]<PERSONID>
      		sPersonID = "P" +ESC+ PERSON.PERSONID
      		
      		// Adds the person
      		SchedulerAddChildResource(SCH_SchedulerBreak..FullName, sGroupID, PERSON.NAME +CR+ gFontItalic(True) + "("+PERSON.EXTENSION+")" + gFontItalic(False) + gLink(sPersonID))
      		
      	end
      	
      end
      
      // Browses the tasks
      for each TASK on STARTDATE
      	
      	// Finds the person linked to the task
      	HReadSeekFirst(PERSON, PERSONID, TASK.PERSONID)
      	if HFound(PERSON) then
      		
      		// Adds the appointment
      		// /!\ Caution: the resource being added by gLink, this same gLink must be specified as resource for the task
      		SchedulerAddAppointment(SCH_SchedulerBreak..FullName, "P"+ESC+PERSON.PERSONID, TASK.STCTASK, TASK.STARTDATE, TASK.ENDDATE, PERSON.GROUPID)	
      		
      	end
      	
      end
      
      // Positions the scheduler on the last resource displayed and stored
      if sDisplayedResource <> "" then SchedulerPositionResource(SCH_SchedulerBreak, sDisplayedResource)
      
      // Re-enables the display of the control
      SCH_SchedulerBreak..DisplayEnabled = True
     type : 458752
   -
     name : SchedulerAddAppointment
     internal_properties : 
     procedure_id : 2014685149002700180
     type_code : 14
     group : 2
     code : |1+
      // Summary: Adds an appointment to the scheduler
      // Syntax:
      //[ <Result> = ] SchedulerAddAppointment (<sNameSchedulerControl> is string, <sResource> is string, <sTitle> is string, <dtStartDate> is datetime, <dtEndDate> is datetime [, <vCategory> is variant])
      //
      // Parameters:
      //	sNameSchedulerControl (ANSI string): Name of the scheduler control
      //	sResource (ANSI string): Resource
      //	sTitle (ANSI string): Title of the appointment
      //	dtStartDate (datetime): Start date and time
      //	dtEndDate (datetime): End date and time
      //	vCategory (variant - default value=""): Category
      // Return value:
      // 	integer: Addition subscript
      //// Automatic process of errors: RESULT -1
      //
      
      Procédure SchedulerAddAppointment(local sNameSchedulerControl is string, local sResource is string, local sTitle is string, local dtStartDate is datetime, local dtEndDate is datetime, local vCategory is variant = "") : int
      
      
      // If the resource specified for the appointment is a child resource
      if gaaChildResource[sResource]..Empty = False then
      	
      	// If the parent resource is collapsed
      	if SchedulerResourceStatus(sNameSchedulerControl, gaaChildResource[sResource]) = resoCollapsed then
      		
      		// The appointment must not be displayed
      		result -1
      
      	end
      	
      end
      
      // Adds the appointment
      result WL.SchedulerAddAppointment(sNameSchedulerControl, sResource, sTitle, dtStartDate, dtEndDate, vCategory)
     type : 458752
