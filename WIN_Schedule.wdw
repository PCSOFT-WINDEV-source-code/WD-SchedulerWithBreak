         Procédure BeforeCreation(aptCreate is an Appointment)
         
         // Stores the appointment that was created
         // (if it is moved onto a "break" resource, it must be repositioned)
         gTempAPT = aptCreate
         
         // Stores the appointment
         HReset(TASK)
         TASK.STARTDATE = aptCreate..StartDate
         TASK.ENDDATE = aptCreate..EndDate
         TASK.STCTASK = aptCreate..Title
         TASK.PERSONID = ExtractString(aptCreate..Resource, 2, ESC)
         if HAdd(TASK) = False then
         	Error("Unable to store the appoinment in database", HErrorInfo(hErrMessage))
         	result False
         end
         
         // Stores the identifier of the appointment
         aptCreate..ID = TASK.TASKID
         
         // Modifies the category
         HReadSeekFirst(PERSON, PERSONID, ExtractString(aptCreate..Resource, 2, ESC))
         if HFound(PERSON)
         	aptCreate..Category = PERSON.GROUPID
         end
         
         result True
