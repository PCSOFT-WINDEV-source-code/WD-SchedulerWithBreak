      // Summary: Adds a child resource
      // Syntax:
      //[ <Result> = ] SchedulerAddChildResource (<sNameSchedulerControl> is string, <sParentID> is string, <sResource> is string)
      //
      // Parameters:
      //	sNameSchedulerControl (ANSI string): Full name of the Scheduler control
      //	sParentID (ANSI string): Identifier of parent resource (caption or gLink)
      //	sResource (ANSI string): Resource (caption with gLink eventually)
      // Return value:
      // 	integer: Subscript of the resource 
      //// Automatic process of errors: RESULT -1
      //
      
      Procédure SchedulerAddChildResource(local sNameSchedulerControl is string, local sParentID is string, local sResource is string) : int
      
      nSubscript 	is int
      sCaption	is string
      sID			is string
      
      
      // The Scheduler control must exist
      if ControlExist(sNameSchedulerControl) = False then
      	ErrorBuild("The Scheduler control %1 does not exist", sNameSchedulerControl)
      	result -1
      end
      
      
      // Defines the caption and the identifier of the resource (if the specified resource contains gLink)
      if Position(sResource, ESC+"K") > 0 then
      	
      	// Retrieves the caption and the identifier (gLink) according to the position of gLink
      	if ExtractString(sResource, 1, ESC+"K") ~= "" then
      		// Caption
      		sCaption 	= ExtractString(sResource, 3, ESC+"K")
      		// Identifier
      		sID 		= ExtractString(sResource, 2, ESC+"K")
      	else
      		// Caption
      		sCaption 	= ExtractString(sResource, 1, ESC+"K")
      		// Identifier
      		sID 		= ExtractString(sResource, 2, ESC+"K")
      	end
      	
      else // no gLink
      	
      	// The caption and the identifier are identical
      	sID 		= sResource
      	sCaption 	= sResource
      	
      end
      
      // Stores for the parent/child couple
      gaaChildResource[sID] = sParentID
      
      // If the parent is collapsed, the resource is not added to the scheduler
      if gaaCollapseBreak[sParentID] = False then result -1
      
      // Adds the resource
      nSubscript = SchedulerAddResource(sNameSchedulerControl, sResource)
      
      
      result nSubscript
