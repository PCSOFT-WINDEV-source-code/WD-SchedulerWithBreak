      -
        code : |1-
         
         // MANAGE THE CURSOR
         
         // Retrieves the identifier of the hovered resource
         sResourceID is string = SchedulerInfoXY(MySelf, schResource, MouseXPos(), MouseYPos())
         switch SchedulerResourceStatus(MySelf..FullName, sResourceID)
         	
         	// It is a group ("break")
         	case resoCollapsed, resoExpanded	
         		
         		// Prevents from selecting a period
         		MySelf..PeriodSelection = False
         		
         		// If the left click is pressed
         		if gbMoveInProgress then
         			
         			// Changes the cursor into forbidden cursor
         			MySelf..MouseCursor = curForbidden
         			
         		else
         			
         			// If the rollover is positioned on the resource area (on the left)
         			if MouseXPos() <= gnResourceWidth _and_ MouseYPos() > gnDayHeight then
         				
         				// Positions the hand cursor (to indicate that a click is possible
         				MySelf..MouseCursor = curSystemHand
         				
         			else // If the rollover is positioned on the appointment area
         				
         				// Positions the default cursor
         				MySelf..MouseCursor = curArrow
         				
         			end
         			
         		end
         		
         	// It is a person ("non break" resource)
         	case resoError
         		
         		// Enables the selection of period
         		MySelf..PeriodSelection = True
         		
         		// If the rollover is positioned on the resource area (on the left)
         		if MouseXPos() <= gnResourceWidth _and_ MouseYPos() > gnDayHeight then
         			
         			// Positions the hand cursor (to indicate that a click is possible
         			MySelf..MouseCursor = curSystemHand
         			
         		else // If the rollover is positioned on the appointment area
         			
         			// Positions the default cursor
         			MySelf..MouseCursor = curArrow
         			
         		end
         		
         	other case
         		
         end
