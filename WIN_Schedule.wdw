      // Summary: Adds a "Break" resource
      // Syntax:
      //[ <Result> = ] SchedulerAddBreakResource (<sNameSchedulerControl> is string, <sResource> is string [, <nColor> is int])
      //
      // Parameters:
      //	sNameSchedulerControl (ANSI string): Full name of the Scheduler control
      //	sResource (ANSI string): Resource (caption possibly containing gLink)
      //	nColor (integer - default value=8421504): Color that must be applied to the "break"
      // Return value:
      // 	integer: Subscript of added resource
      //// Automatic process of errors: RESULT -1
      //
      
      Procédure SchedulerAddBreakResource(local sNameSchedulerControl is string, local sResource is string, local nColor is int = RGB(190, 191, 191)) : int
      
      nSubscript 	is int
      sID			is string
      sCaption	is string
      
      
      // The Scheduler control must exist
      if ControlExist(sNameSchedulerControl) = False then
      	ErrorBuild("The Scheduler control %1 does not exist", sNameSchedulerControl)
      	result -1
      end
      
      // Defines the caption and the identifier of the resource (if the specified resource contains gLink)
      if Position(sResource, ESC+"K") > 0 then
      	
      	// Retrieves the caption and the identifier (gLink) according to the position of gLink
      	if ExtractString(sResource, 1, ESC+"K") ~= "" then
      		// Caption
      		sCaption 	= ExtractString(sResource, 3, ESC+"K")
      		// Identifier
      		sID 		= ExtractString(sResource, 2, ESC+"K")
      	else
      		// Caption
      		sCaption 	= ExtractString(sResource, 1, ESC+"K")
      		// Identifier
      		sID 		= ExtractString(sResource, 2, ESC+"K")
      	end
      	
      else // no gLink
      	
      	// The caption and the identifier are identical
      	sID 		= sResource
      	sCaption 	= sResource
      	
      end
      
      // Adds the resource
      nSubscript = SchedulerAddResource(sNameSchedulerControl, sResource)
      
      // Modifies the display of the group to "simulate" a break (reduced height, different color)
      {sNameSchedulerControl, indControl}..Resource[sID]..Height 				= 20
      {sNameSchedulerControl, indControl}..Resource[sID]..BackgroundColor 			= nColor
      {sNameSchedulerControl, indControl}..Resource[sID]..TitleBackgroundColor 	= nColor
      
      // If it is the first load operation: stores the "break" as expanded by default
      if gaaCollapseBreak[sID]..Empty then gaaCollapseBreak[sID] = True
      
      // According to the colllapse status of the "break" to apply
      switch gaaCollapseBreak[sID]
      	// Expanded
      	case True
      		// Adds an expanded symbol to the title
      		{sNameSchedulerControl, indControl}..Resource[sID]..Title = gImage(IMG_Expanded) +" "+ sCaption
      		
      	// Collapsed
      	case False
      		// Adds a collapsed symbol to the title
      		{sNameSchedulerControl, indControl}..Resource[sID]..Title = gImage(IMG_Collapsed) +" "+ sCaption
      		
      end
      
      result nSubscript
     type : 458752
